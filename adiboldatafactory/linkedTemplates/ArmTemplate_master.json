{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adiboldatafactory"
		},
		"AzureDataLakeStorage_linkedservice_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage_linkedservice'"
		},
		"SQL_DB_LINKED_SERVICE_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SQL_DB_LINKED_SERVICE'"
		},
		"AzureDataLakeStorage_linkedservice_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adiboladls.dfs.core.windows.net/"
		},
		"SQL_DB_LINKED_SERVICE_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "adibolsqlserver.database.windows.net"
		},
		"SQL_DB_LINKED_SERVICE_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "adibolsqlserver"
		},
		"SQL_DB_LINKED_SERVICE_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "aditya274"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adiboldatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage_linkedservice_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_linkedservice_accountKey')]"
					},
					"SQL_DB_LINKED_SERVICE_password": {
						"value": "[parameters('SQL_DB_LINKED_SERVICE_password')]"
					},
					"AzureDataLakeStorage_linkedservice_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_linkedservice_properties_typeProperties_url')]"
					},
					"SQL_DB_LINKED_SERVICE_properties_typeProperties_server": {
						"value": "[parameters('SQL_DB_LINKED_SERVICE_properties_typeProperties_server')]"
					},
					"SQL_DB_LINKED_SERVICE_properties_typeProperties_database": {
						"value": "[parameters('SQL_DB_LINKED_SERVICE_properties_typeProperties_database')]"
					},
					"SQL_DB_LINKED_SERVICE_properties_typeProperties_userName": {
						"value": "[parameters('SQL_DB_LINKED_SERVICE_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adiboldatafactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adiboldatafactory_ArmTemplate_0')]"
			]
		}
	]
}